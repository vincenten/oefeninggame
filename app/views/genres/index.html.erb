<h1>Listing genres</h1>

<table>
  <thead>
    <tr>
     <%# <th>Genre</th>%>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @genres.each do |genre| %>
      <tr>
        <%#<td><%= genre.genre_id %><%#</td>%>
        <td><%= genre.name %></td>
        <td><%= link_to genre.description, genre %></td>
     
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Genre', new_genre_path %>
